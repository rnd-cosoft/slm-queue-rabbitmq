{
    "name": "rnd-cosoft/slm-queue-rabbitmq",
    "description": "Laminas Framework module that integrates RabbitMQ as queuing system",
    "license": "MIT",
    "type": "laminas-module",
    "keywords": [
        "laminas",
        "queue",
        "job",
        "rabbitmq"
    ],
    "homepage": "https://github.com/rnd-cosoft/slm-queue-rabbitmq",
    "config": {
        "process-timeout": 900,
        "bin-dir": "bin/"
    },
    "require": {
        "php": "^7.1",
        "laminas/laminas-console": "^2.7",
        "laminas/laminas-modulemanager": "^2.8",
        "laminas/laminas-stdlib": "^3.2",
        "laminas/laminas-eventmanager": "^3.2",
        "laminas/laminas-servicemanager": "^3.3",
        "laminas/laminas-mvc": "^3.1",
        "container-interop/container-interop": "^1.2",
        "php-amqplib/php-amqplib": "^2.7",
        "slm/queue": "^2.0",
        "psr/log": "^1.0",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "SlmQueueRabbitMq\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlmQueueRabbitMqTest\\": "test"
        }
    }
}
